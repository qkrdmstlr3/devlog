{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@Config": "src/config",
          "@ContextAPI": "src/contextAPI",
          "@Components": "./src/components",
          "@Hooks": "./src/hooks",
          "@Style": "./src/common/style",
          "@Utils": "./src/utils",
          "@Libs": "./src/libs",
          "@RenderingData": "./src/common/rendering-data"
        }
      }
    ]
  ]
}
