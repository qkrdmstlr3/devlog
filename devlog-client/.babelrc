{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@Config": ["src/config"],
          "@ContextAPI": ["src/contextAPI"],
          "@Components": "./src/components",
          "@Style": "./src/common/style",
          "@Utils": "./src/utils"
        }
      }
    ]
  ]
}